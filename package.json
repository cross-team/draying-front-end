{
  "name": "draying-front-end",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "build:serve": "gatsby clean && gatsby build && gatsby serve -p 8000",
    "clean": "gatsby clean",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "jest",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test build:serve http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test build:serve http://localhost:8000 cy:run",
    "start:test-server": "start-server-and-test build:serve http://localhost:8000"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@cross.team/gatsby-theme-eslint": "^1.0.1",
