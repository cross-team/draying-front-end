{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "build:serve": "gatsby clean && gatsby build && gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@cross.team/gatsby-theme-eslint": "^1.0.1",
    "@cross.team/gatsby-theme-material-ui": "^1.1.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "gatsby": "^2.18.12",
    "gatsby-plugin-create-client-paths": "^2.1.20",
    "gatsby-plugin-offline": "^3.0.29",
    "gatsby-plugin-react-axe": "^0.3.0",
    "gatsby-plugin-react-helmet": "^3.1.18",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "apollo": "^2.21.2",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
